type=task
name=build-simply-ci
trigger.type=REPOSITORY_UPDATED
trigger.repository=simply-ci
workspace.type=CLONED_REPOSITORY
workspace.repository=simply-ci
script=#!/bin/bash\n\
set -e\n\
cd simply-ci\n\
export JAVA_HOME=/Library/Java/JavaVirtualMachines/jdk1.8.0_25.jdk/Contents/Home/\n\
mvn clean compile -DskipTests\n
